#!/bin/bash

cd simulations;

python experiment.py  \
	--numClients 1 \
	--numServers 2 \
	--numWorkload 1 \
	--workloadModel constant \
	--serverConcurrency 1 \
	--serviceTime 4.00\
	--utilization 0.90 \
	--serviceTimeModel constant \
	--replicationFactor 3 \
	--selectionStrategy pending \
	--shadowReadRatio 0.0 \
	--accessPattern uniform \
	--nwLatencyBase 0.00 \
	--nwLatencyMu 0.0 \
	--nwLatencySigma 0.0 \
	--expPrefix oracle\
	--simulationDuration 100000\
	--seed 433\
	--numRequests 10000\
	--logFolder logs\
	--expScenario base\
	--demandSkew 0\
	--intervalParam 500\
	--rangeParam 10;

python experiment.py  \
 	--numClients 1 \
	--numServers 2 \
	--numWorkload 1 \
	--workloadModel constant \
	--serverConcurrency 1 \
	--serviceTime 4.00 \
	--utilization 0.90 \
	--serviceTimeModel constant \
	--replicationFactor 3 \
	--backpressure \
	--rateInterval 20\
	--cubicC 0.000004\
	--cubicSmax 10\
	--cubicBeta 0.2\
	--hysterisisFactor 2\
	--selectionStrategy expDelay \
	--shadowReadRatio 0.0 \
	--accessPattern uniform \
	--nwLatencyBase 0.00 \
	--nwLatencyMu 0.0 \
	--nwLatencySigma 0.0 \
	--expPrefix expDelay \
	--simulationDuration 100000 \
	--seed 433 \
	--numRequests 10000\
	--logFolder logs\
	--expScenario base\
	--demandSkew 0\
	--intervalParam 500\
	--rangeParam 10;


cd ..
cd plotting;
# Rscript timeseries.r primary;
# Rscript timeseries.r rp;
# Rscript timeseries.r pending;
