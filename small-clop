#!/bin/bash

cd simulations;

python experiment.py  \
	--numClients 60 \
	--numServers 10 \
	--numWorkload 1 \
	--workloadModel poisson \
	--serverConcurrency 1 \
	--serviceTime 4.00\
	--utilization 0.45 \
	--serviceTimeModel random.expovariate \
	--replicationFactor 3 \
	--selectionStrategy random \
	--shadowReadRatio 0.0 \
	--accessPattern zipfian \
	--nwLatencyBase 0.00 \
	--nwLatencyMu 0.0\
	--nwLatencySigma 0.00 \
	--expPrefix oracle\
	--simulationDuration 100000\
	--seed 2\
	--numRequests 10000\
	--logFolder logs\
	--expScenario heterogenousStaticServiceTimeScenario\
	--demandSkew 0.0\
	--highDemandFraction 0.0\
	--slowServerFraction 0.3\
	--slowServerSlowness 0.8\
	--intervalParam 100\
	--timeVaryingDrift 10;

python experiment.py  \
 	--numClients 60 \
	--numServers 10 \
	--numWorkload 1 \
	--workloadModel poisson \
	--serverConcurrency 1 \
	--serviceTime 4.00 \
	--utilization 0.45 \
	--serviceTimeModel random.expovariate \
	--replicationFactor 3 \
	--backpressure \
	--rateInterval 20\
	--cubicC 0.000004\
	--cubicSmax 10\
	--cubicBeta 0.2\
	--hysterisisFactor 2\
	--selectionStrategy expDelay \
	--shadowReadRatio 0.0 \
	--accessPattern zipfian \
	--nwLatencyBase 0.00 \
	--nwLatencyMu 0.0\
	--nwLatencySigma 0.00 \
	--expPrefix expDelay \
	--simulationDuration 100000 \
	--seed 2 \
	--numRequests 10000\
	--logFolder logs\
	--expScenario heterogenousStaticServiceTimeScenario\
	--demandSkew 0\
	--highDemandFraction 0.0\
	--slowServerFraction 0.3\
	--slowServerSlowness 0.8\
	--intervalParam 100\
	--timeVaryingDrift 10;


cd ..
cd plotting;
# Rscript timeseries.r primary;
# Rscript timeseries.r rp;
# Rscript timeseries.r pending;
# Rscript timeseries.r pendingXserviceTime;
Rscript timeseries.r oracle;
Rscript timeseries.r expDelay;
cd ..
